arorainstance=/root/arora/arora.`/root/free_arora.sh`

url=`cat /mnt/swshare/${SCREEN_ID}_${media_id}_${W}x${H}.url | head -1`

${arorainstance} \
   -geometry ${W}x${H}+${X}+${Y} \
   ${url} \
   2>&1 >/dev/null && date +"%H:%M:%S E${event_id}: Finished" >> ${LOGFILE} &

ppid=${!}
event_pid_a[${event_id}]=`ps -eo pid,ppid | grep ${ppid} | tail -1 | cut -d" " -f1`

echo "=== URL ===" >> ${LOGFILE}
echo ${url} >> ${LOGFILE}
echo ${ppid} >> ${LOGFILE}

